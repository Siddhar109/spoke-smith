# OpenAI API Configuration
OPENAI_API_KEY=sk-your-api-key-here

# Realtime Model Configuration
# Default: gpt-4o-realtime-preview-2024-12-17 (beta)
# GA model: gpt-4o-realtime (when available)
OPENAI_REALTIME_MODEL=gpt-4o-realtime-preview-2024-12-17
OPENAI_TRANSCRIPTION_MODEL=gpt-4o-mini-transcribe

# Frontend URL Configuration
NEXT_PUBLIC_API_URL=http://localhost:8000

# Face coaching (optional)
# Enable Phase B phrasing + (optionally) Phase C keyframe verification
NEXT_PUBLIC_FACE_PHRASE_MODEL_ENABLED=false
# Text-only model for short nudge rewrites (Phase B)
OPENAI_FACE_PHRASE_MODEL=gpt-4o-mini
# Vision-capable model for keyframe verification (Phase C); defaults to OPENAI_FACE_PHRASE_MODEL if unset
# OPENAI_FACE_VERIFY_MODEL=gpt-4o-mini
# FACE_NUDGE_DEFAULT_COOLDOWN_MS=12000

# Session Management (optional)
KAWKAI_KEEP_SESSION_AUDIO=false
